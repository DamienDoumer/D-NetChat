<Window x:Class="DoumeraNetChat.EditUserSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoumeraNetChat"
        mc:Ignorable="d"
        Title="EditUserSettings" Background="{DynamicResource MainBackGround}" Height="189.049" Width="387.197" ResizeMode="NoResize" Icon="Icons\Doumera Icon.png">
    <Grid Margin="1">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="20" Margin="102,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="241"/>
        <Button x:Name="OKbutton" Style="{DynamicResource GlassButtonStyle}" Click="OKbutton_Click" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="268,107,0,0" Height="24"/>
        <Label x:Name="label" Content="User Name:" HorizontalAlignment="Left" Margin="10,19,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="An IP address will automatically be given to you"
               HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="333"/>
    </Grid>
</Window>
